#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    
    int a[n+1],b[n+1];
    
    set<int> s;
    
    for(int i=0;i<n;i++)
    {
        cin >> a[i];
        s.insert(a[i]);
    }
    
    for(int i=0;i<n;i++)
    {
        cin >> b[i];
        s.insert(b[i]);
    }
    
    for(int c:s)
    {
        cout << c << ' ';
    }
    
}
