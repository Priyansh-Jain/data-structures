#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n ;
    cin >> n;
    
    int ar[n+1],a=0,b=0;
    
    for(int i=1;i<n;i++)
    {
        cin >> ar[i];
        a=a^ar[i];
    }
    
    for(int i=1;i<=n;i++)
    {
        b=b^i;
    }
    int c=a^b;
    
    cout << c << endl;
}
